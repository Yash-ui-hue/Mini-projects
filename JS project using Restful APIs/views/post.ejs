<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="/style.css">
</head>
<body>
    <% for(post of posts) { %>
        <% if(id==post.id) { %>
            <div class="specificpost">
                <h2>Username: 
                    @<%=post.username%>
                </h2>  
                
                
                <h3>Content: <br> <%=post.content%></h3>
            </div>
            
        <%}%>
    <% } %>
    <h2></h2>
    
</body>

</html>